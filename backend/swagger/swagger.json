{
    "swagger": "2.0",
    "info": {
        "title": "Finance manager",
        "contact": {},
        "version": "0.0.1"
    },
    "paths": {
        "/auth/google_auth": {
            "post": {
                "description": "Exchanges a Google OAuth token for an app-specific JWT",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Auth"
                ],
                "summary": "Authenticate using a Google token",
                "operationId": "googleAuth",
                "parameters": [
                    {
                        "description": "The google token request. Probably received from google oAuth",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/rest.TokenRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/rest.UserResponseDTO"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "rest.TokenRequest": {
            "type": "object",
            "properties": {
                "idToken": {
                    "type": "string"
                }
            }
        },
        "rest.UserResponseDTO": {
            "type": "object",
            "properties": {
                "firstName": {
                    "type": "string"
                },
                "jwtToken": {
                    "type": "string"
                },
                "lastName": {
                    "type": "string"
                },
                "pictureUrl": {
                    "type": "string"
                },
                "userEmail": {
                    "type": "string"
                }
            }
        }
    }
}